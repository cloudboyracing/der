          "_name": "list1",
          "_sourceTheme": "gridm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"list1\" group=\"List\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-switch-arrow, mobirise-icons2\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"7\" max=\"12\" value=\"12\" step=\"1\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Arrow\" name=\"showArrow\" checked>\n        <input type=\"color\" title=\"Color\" name=\"arrowsColor\" value=\"#ffffff\" condition=\"showArrow\" selected>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <select title=\"Amount\" name=\"cardAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../_images/background3.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#141414\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container-fluid\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-{{textWidth}}\">\n                <div class=\"wrapper\">\n                    <div class=\"mbr-section-head align-center\" mbr-if=\"showTitle\">\n                        <h3 class=\"mbr-section-title mb-0 mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\">\n                            <p class=\"mb-0\"><b>Open</b>Roles</p>\n                        </h3>\n                    </div>\n                    <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n\n                        <div class=\"card\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n                                    <div class=\"panel-title__container\">\n                                        <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" data-app-selector=\".panel-title-edit\">\n                                            <p class=\"panel-title-text\"><b>Director</b></p>\n                                        </h6>\n                                        <div class=\"panel-title__text-container\">\n                                            <div class=\"panel-title__text-item\">\n                                                <p class=\"mbr-fonts-style panel-text-add\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text-add\">\n                                                    Remote\n                                                </p>\n                                                <p class=\"mbr-fonts-style panel-add\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-add\">\n                                                    location\n                                                </p>\n                                            </div>\n                                            <div class=\"panel-title__text-item\">\n                                                <p class=\"mbr-fonts-style panel-text-add\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-text-add\">\n                                                    1234-12345$\n                                                </p>\n                                                <p class=\"mbr-fonts-style panel-add\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-add\">\n                                                    salary\n                                                </p>\n                                            </div>\n                                            <div class=\"panel-title__text-item\">\n                                                <p class=\"mbr-fonts-style panel-text-add\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text-add\">\n                                                    High\n                                                </p>\n                                                <p class=\"mbr-fonts-style panel-add\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-add\">\n                                                    level\n                                                </p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <span class=\